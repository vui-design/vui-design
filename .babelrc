{
  "presets": [
    [
      "env",
      {
        "modules": false,
        "targets": {
          "browsers": [
            "> 1%",
            "last 2 versions",
            "not ie <= 8"
          ]
        }
      }
    ],
    "stage-2"
  ],
  "plugins": [
    "transform-class-properties",
    "transform-object-assign",
    "transform-object-rest-spread",
    "transform-runtime",
    "transform-vue-jsx"
  ]
}